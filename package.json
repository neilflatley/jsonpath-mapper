{
  "name": "jsonpath-mapper",
  "version": "1.1.0",
  "description": "A json to json transformation utility with a few nice features to use when translating for example API responses into a domain object for use in your domain-driven JavaScript applications. Can be used in React applications with the 'useMapper' hook.",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "docs": "https://github.com/dchester/jsonpath/blob/master/README.md",
  "scripts": {
    "build": "rimraf ./dist && tsc && rollup -c",
    "prestart": "npm run build",
    "start": "npm run test",
    "test": "mocha --es-module-specifier-resolution=node --require @babel/register --timeout 500000 tests/json-mapper.tests.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neilflatley/jsonpath-mapper.git"
  },
  "keywords": [
    "jsonpath",
    "jpath",
    "json mapper",
    "json transformation",
    "json transformer",
    "json to json",
    "json2json",
    "object mapping",
    "prop mapping",
    "json",
    "mapper",
    "mapping",
    "transform",
    "transformer",
    "format",
    "formatting",
    "mapping hook",
    "usemapper",
    "use mapper"
  ],
  "author": "Neil Flatley",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neilflatley/jsonpath-mapper/issues"
  },
  "homepage": "https://github.com/neilflatley/jsonpath-mapper#readme",
  "dependencies": {
    "fromentries": "^1.3.2",
    "jsonpath-plus": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/register": "^7.25.9",
    "chai": "^5.1.2",
    "mocha": "^10.7.3",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^2.40.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "tslint": "^6.1.3",
    "typescript": "^5.6.3"
  }
}
